# Example Environment Variables
# Copy this file to .env and fill in your values

# Sketchfab API Key (optional)
# Get your key from: https://sketchfab.com/developers
# Used for AI-powered model search
VITE_SKETCHFAB_API_KEY=your_api_key_here

# Development Mode (optional)
# Set to 'true' to enable verbose logging
VITE_DEV_MODE=false

# Camera Resolution (optional)
# Adjust based on device performance
VITE_CAMERA_WIDTH=1920
VITE_CAMERA_HEIGHT=1080

# ML Update Interval (optional)
# How often to run surface detection (in milliseconds)
VITE_ML_UPDATE_INTERVAL=2000
